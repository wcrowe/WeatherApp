{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "func-names": 0,
    "eol-last": 0,
    "arrow-body-style": ["error", "always"]
  },
  "plugins": [
    "react",
  ]
}
